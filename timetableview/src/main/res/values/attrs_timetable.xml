<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TimetableView">
        <attr name="row_count" format="integer" />
        <attr name="column_count" format="integer" />
        <attr name="cell_height" format="dimension" />
        <attr name="side_cell_width" format="dimension" />
        <attr name="header_title" format="reference" />
        <attr name="sticker_colors" format="reference" />
        <attr name="start_time" format="integer" />
        <attr name="header_highlight_color" format="color" />
        <attr name="color_surface" format="color" />
        <attr name="color_borders" format="color" />
        <attr name="side_color_text" format="color" />
        <attr name="color_side" format="color" />
        <attr name="color_side_header_text" format="color" />
        <attr name="border_width" format="enum">
            <enum name="noWidth" value="0" />
            <enum name="light" value="1" />
            <enum name="normal" value="2" />
            <enum name="thick" value="3" />
            <enum name="extra_thick" value="4" />
        </attr>
        <attr name="header_highlight_type" format="enum">
            <enum name="color" value="0" />
            <enum name="image" value="1" />
        </attr>
        <attr name="header_highlight_image_size" format="dimension" />
        <attr name="header_highlight_image" format="reference" />
    </declare-styleable>
</resources>